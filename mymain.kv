
#include Global.py

windowmanager:
    id: "windowmanager"
    MainMenu:
    firstquestion:
    secondquestion:
    thirdquestion:
    fourthquestion:
    fifthquestion:
    sixthquestion:
    seventhquestion:
    eighthquestion:
    ninethquestion:
    tenthquestion:
    Login_Screen:
    Register_Screen:
    correct_screen:
    incorrect_screen:
    mode:
    results:
    leaderboard:




<MainMenu>:
    name: "Main"
    id: "Main"
    canvas:
        Rectangle:
            source:"main_menu_bg.jpg"
            pos: self.pos
            size: self.size



    MDLabel:
        halign: 'center'
        pos_hint: {"center_x": 0.5, "center_y": 0.9}
        text: "Computer Science Quiz!"
        font_size: 40

    MDRoundFlatButton:    
        pos_hint: {"center_x": 0.5, "center_y": 0.6}
        text: "Login"
        on_press:
            app.root.current = "Login"

    MDRoundFlatButton:

        pos_hint: {"center_x": 0.5, "center_y": 0.35}
        text: "Register"
        on_press:
            app.root.current = "Register"



<Login_Screen>:
    name: "Login"
    id: "Login"
    canvas:
        Rectangle:
            source:"main_menu_bg.jpg"
            pos: self.pos
            size: self.size

    MDCard:
        size_hint: None, None
        size: 300, 400
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        elevation: 10
        padding: 25
        spacing: 25
        orientation: "vertical"

        MDLabel:
            id: label
            text: "Please Log in"
            font_size: 30
            halign: "center"
            size_hint_y: None
            height: self.texture_size[1]
            padding_y: 15

        MDTextFieldRound:
            id: user
            hint_text: "Username"
            icon_right: "account"
            size_hint_x: None
            width: 200
            font_size: 18
            pos_hint: {"center_x": 0.5}
            normal_color: app.theme_cls.accent_color
            color_active:app.theme_cls.accent_color

        MDTextFieldRound:
            id: password
            hint_text: "Password"
            icon_right: "eye-off"
            size_hint_x: None
            width: 200
            font_size: 18
            pos_hint: {"center_x": 0.5}
            normal_color: app.theme_cls.accent_color
            color_active:app.theme_cls.accent_color
            password: True

        MDRoundFlatButton:
            id: login_button
            text: "Log in"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            on_press:
                root.login()


        MDRoundFlatButton:
            text: "Clear"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            on_press:
                root.clear()

        MDRoundFlatButton:
            text: "Back"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            on_press: 
                app.root.current = "Main"



<Register_Screen>:
    name: "Register"
    id: "Register"
    canvas:
        Rectangle:
            source:"main_menu_bg.jpg"
            pos: self.pos
            size: self.size

    MDCard:
        size_hint: None, None
        size: 300, 400
        pos_hint: {"center_x": 0.5, "center_y": 0.4}
        elevation: 10
        padding: 25
        spacing: 25
        orientation: "vertical"

        MDLabel:
            id: register_label
            text: "Please enter your details"
            font_size: 30
            halign: "center"
            size_hint_y: None
            height: self.texture_size[1]
            padding_y: 15

        MDTextFieldRound:
            id: register_user
            hint_text: "Username"
            icon_right: "account"
            size_hint_x: None
            width: 200
            font_size: 18
            pos_hint: {"center_x": 0.5}
            normal_color: app.theme_cls.accent_color
            color_active:app.theme_cls.accent_color

        MDTextFieldRound:
            id: register_password
            hint_text: "Password"
            icon_right: "eye-off"
            size_hint_x: None
            width: 200
            font_size: 18
            pos_hint: {"center_x": 0.5}
            normal_color: app.theme_cls.accent_color
            color_active:app.theme_cls.accent_color
            password: True

        MDRoundFlatButton:
            id: register_button
            text: "Register"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            on_press:
                root.register()


        MDRoundFlatButton:
            text: "Clear"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            on_press:
                root.clear()

        MDRoundFlatButton:
            text: "Back"
            font_size: 12
            pos_hint: {"center_x": 0.5}
            on_press: 
                app.root.current = "Main"

             
<mode>:
    name: "Mode"
    id: "Mode"
    canvas:
        Rectangle:
            source:"main_menu_bg.jpg"
            pos: self.pos
            size: self.size

