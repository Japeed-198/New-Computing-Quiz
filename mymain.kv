            size: self.size

    MDLabel:
        halign: 'center'
        pos_hint: {"center_x": 0.5, "center_y": 0.9}
        text: "Correct!"
        font_size: 40

    MDLabel:
        id: score_counter
        halign: 'center'
        pos_hint: {"center_x": 0.5, "center_y": 0.5}
        font_size: 40

    MDRoundFlatButton:
        id: next_screen 
        text: "Next"
        font_size: 12
        pos_hint: {"center_x": 0.5, "center_y": 0.3}


