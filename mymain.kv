            root.easy_mode()

    MDRoundFlatButton:
        id: hard
        text: "Hard"
        font_size: 30
        pos_hint: {"center_x": 0.5, "center_y": 0.3}
        on_press:
            root.hard_mode()


<correct_screen>:
    name: "Correct"
    id: "Correct"
    on_enter: root.display_score_counter()
